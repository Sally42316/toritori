<h1>Admin::Users#index</h1>
<p>Find me in app/views/admin/users/index.html.erb</p>

<h1>一般ユーザー一覧</h1>
<table>
  <thead>
    <tr>
      <th>ユーザー名</th>
      <th>詳細</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td> <!-- ユーザー名 -->
        <td><%= link_to '詳細', admin_user_path(user) %></td> <!-- ユーザー詳細ページへのリンク -->
      </tr>
    <% end %>
  </tbody>
</table>