<h1>Welcome to the Post Feed</h1>

<% @posts.each do |post| %>
  <div class="post">
    <p><strong><%= post.user.email %></strong> posted:</p>
    <p><%= post.text %></p>  
    <p><%= post.created_at.strftime("%Y-%m-%d %H:%M") %></p>
  </div>
<% end %>