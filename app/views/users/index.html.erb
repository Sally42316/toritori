<h1>Users#index</h1>
<p>Find me in app/views/users/index.html.erb</p>

<h1>User List</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td>
          <%= link_to 'Show', user_path(user) %> 
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Back to Home', root_path %>
